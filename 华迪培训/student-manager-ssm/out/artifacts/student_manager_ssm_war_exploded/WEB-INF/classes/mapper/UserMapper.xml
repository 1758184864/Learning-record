<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mapper.UserMapper">

    <select id="findInfoByUsername" resultType="User">
        select * from `student-manager-ssm`.user where username = #{username}
    </select>

</mapper>